{
  "context_servers": {
    "mcp-server-github": {
      "source": "extension",
      "enabled": true,
      "settings": {
        "github_personal_access_token": "$GITHUB_TOKEN"
      }
    }
  },
  "agent": {
    "play_sound_when_agent_done": true
  },
  "autosave": "on_focus_change",
  "edit_predictions": {
    "mode": "eager"
  },
  "buffer_font_family": "Hasklug Nerd Font Mono",
  "buffer_font_size": 15,
  "file_types": {
    "Groovy": ["Jenkinsfile", "**/Jenkinsfile"]
  },
  "preferred_line_length": 120,
  "project_panel": {
    "button": true,
    "default_width": 240,
    "dock": "left",
    "entry_spacing": "comfortable",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 20,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true,
    "scrollbar": {
      "show": null
    },
    "show_diagnostics": "all",
    "indent_guides": {
      "show": "always"
    },
    "hide_root": true
  },
  "scrollbar": {
    "show": "auto"
  },
  "tab_size": 2,
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Ayu Dark"
  },
  "ui_font_features": {
    "calt": true
  },
  "ui_font_size": 16,
  "vim_mode": true
}
